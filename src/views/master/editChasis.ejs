<%- include ('partials/_masterHeader'); %>
    <form class="text-center p-3 row container-fluid" action="/editTruck/<%=ramplas._id%>" data-toggle="validator" method="POST">

        <div class="md-form form-sm col-sm-4">
            <input type="string" style="text-transform:uppercase" name="patente" class="form-control form-control-sm" maxlength="6"
                pattern="[A-Za-z]{4||2}[0-9]{2||4}" value="<%=ramplas.patente%>" required>
            <label for="patente">Patente</label>
        </div>

        <!-- Modal footer-->
        <div class="modal-footer container-fluid">
            <a href="/master/chasis"><button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button></a>
            <button type="submit" class="btn btn-primary">Guardar</button>
        </div>
        <!--Fin Modal-->
    </form>
<%- include ('partials/_footer'); %>